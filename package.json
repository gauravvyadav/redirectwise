{
  "name": "redirectwise",
  "description": "Track and analyze HTTP redirect chains for any URL",
  "private": true,
  "license": "UNLICENSED",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:edge": "wxt -b edge",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "build:all": "npm run build && npm run build:firefox && npm run build:edge",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "zip:all": "npm run zip && npm run zip:firefox && npm run zip:edge",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.279",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@wxt-dev/module-react": "^1.1.2",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "typescript": "^5.6.3",
    "wxt": "^0.20.11"
  }
}
